<pw id="B2_今日推荐图文@图酷" />
<list cachetime="1800" action="image" num="3" title="今日推荐图文" />
<div class="tu_dl_img cc">
<!--# $count = 0;#-->
	<loop>
<!--#
$tempClass = (($count % 3) == 0) ? 'class="one"' : '';
$count++;
#-->
	<dl $tempClass>
		<dt><a href="{url}"><img src="{image,180,135}" width="180" height="135" /></a></dt>
		<dd>
			<p class="s6 cc mb10"><span class="imgView">查看：</span><span class="mr10 fl">{hits}</span><span class="imgFav">收藏：</span><span class="fl">{collectnum}</span></p>
			<p class="mb5"><a href="{url}">{title,24}</a></p>
			<p><a href="{authorurl}" class="s6 mr20">{author}</a><span class="s6">{totalnum} 张</span></p>
		</dd>
	</dl>
	</loop>
</div>

