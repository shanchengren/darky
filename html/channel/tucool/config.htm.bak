
<pw id="A1_头部图片轮播@图酷" />
<ul class="tu_flash_switch" id="SlayerA_1">
	<list cachetime="1800" action="image" num="4" title="图片播放器大图" />
	<loop>
		<li class="switchItem" style="display:none"><a href="{url}"  target="_blank"><img src="{image,520,300}" width="520" height="300" /><h3>{title,30}</h3></a></li>
	</loop>
</ul>
<div class="tu_flash_bg"></div>
	<ol class="tu_flash_nav" id="SlayerA_2">
